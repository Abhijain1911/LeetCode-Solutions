class Solution {
    public String restoreString(String s, int[] indices) {
        char[] ch=new char[s.length()];
        for(int i=0;i<indices.length;i++){
            int a=indices[i];
            ch[a]=s.charAt(i);
        }
        StringBuilder sb=new StringBuilder();
        sb.append(ch);
        return sb.toString();
    }
}
